<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="30.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.zavrsniprojektvojvodic.StudentMenuController">
   <children>
      <MenuBar layoutY="4.0" prefHeight="26.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" onAction="#AddExperiment" text="Add an experiment">
            <items>
              <MenuItem mnemonicParsing="false" text="Add" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" onAction="#RemoveExperiment" text="Remove an experiment">
            <items>
              <MenuItem mnemonicParsing="false" text="Remove" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" onAction="#AllExperiment" text="All experiments">
            <items>
              <MenuItem mnemonicParsing="false" text="Show" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" onAction="#LogOut" text="Log out">
              <items>
                <MenuItem mnemonicParsing="false" text="Exit" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
